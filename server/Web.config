<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- DBType optional value: MSSQL, ACCESS  -->
		<add key="DBType" value="ACCESS"/>

		<!-- MSSQL Config -->
		<add key="DBHost" value="127.0.0.1"/>
		<add key="DBName" value="AndroidAppStore"/>
		<add key="DBUser" value="sa"/>
		<add key="DBPwd" value=""/>

		<!-- ACCESS Config -->
		<add key="DBFile" value="D:\Code\hexiaojie\AndroidHelper\server\database\AndroidAppStore.mdb"/>

	</appSettings>

    <connectionStrings>
	</connectionStrings>

    <system.data>
        <DbProviderFactories>
        </DbProviderFactories>
    </system.data>

    <system.web>
		<compilation defaultLanguage="c#" debug="true" />
		<customErrors mode="RemoteOnly" />
		<authorization>
			<allow users="*" />
		</authorization>
		<authentication mode="Forms" />
		<trace
            enabled="false"
            requestLimit="10"
            pageOutput="false"
            traceMode="SortByTime"
            localOnly="true"
        />
		<sessionState
            mode="InProc"
            stateConnectionString="tcpip=127.0.0.1:42424"
            sqlConnectionString="data source=127.0.0.1;user id=sa;password="
            cookieless="false"
            timeout="20"
        />
		<globalization
            requestEncoding="utf-8"
            responseEncoding="utf-8"
        />

    </system.web>
</configuration>
